<%= form_for @changeset, @action, [multipart: true, as: :registration], fn f  -> %>

<div class="form-group">
    <label>Photo</label>
    <%= file_input f, :profile_image, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<%end%>
