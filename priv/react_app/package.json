{
  "name": "nexpo",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "antd": "^3.10.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "react-test-renderer": "^16.5.2",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.77.0",
    "flow-typed": "^2.5.1",
    "nock": "^9.4.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "redux-mock-store": "^1.5.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "lint": "prettier-eslint \"src/**/*.js\"",
    "lint-fix": "prettier-eslint --write \"src/**/*.js\"",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "flow coverage": "flow coverage",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run test-ci",
    "test-js": "react-scripts test",
    "test-watch": "npm-run-all -p watch-css test-js",
    "test-ci": "npm run build-css && CI=true npm run test-js",
    "test-coverage": "npm run test-js -- --coverage",
    "eject": "react-scripts eject",
    "postinstall": "npx flow-typed install"
  },
  "engines": {
    "node": "8.11.1",
    "npm": ">5.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
