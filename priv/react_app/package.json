{
  "name": "nexpo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^2.9.1",
    "material-ui": "^0.18.4",
    "node-sass-chokidar": "^0.0.3",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.7",
    "jest-cli": "^21.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run test-ci",
    "test-js": "react-scripts test --env=jsdom",
    "test-watch": "npm-run-all -p watch-css test-js",
    "test-ci": "npm run build-css && CI=true npm run test-js",
    "eject": "react-scripts eject"
  }
}
